if (keyboard_check(vk_left)) &amp;&amp; (place_free(x-4,y)){
sprite_index = spr_player_left;
x -= walkingSpeed;
image_speed = 0.5;
}

// Checks to see if the up arrow key is pressed and moves 4 pixels up.
if (keyboard_check(vk_up)) &amp;&amp; (place_free(x,y-4)){
sprite_index = spr_player_up;
y -= walkingSpeed;
image_speed = 0.5;
} 

// Checks to see if the right key is pressed and moves 4 pixels to the right.
if (keyboard_check(vk_right)) &amp;&amp; (place_free(x+4,y)){
sprite_index = spr_player_right;
x += walkingSpeed;
image_speed = 0.5;
}

// Checks to see if the down key is pressed and moves 4 pixels down.
if (keyboard_check(vk_down)) &amp;&amp; (place_free(x,y+4)){
sprite_index = spr_player_down;
y += walkingSpeed;
image_speed = 0.5;
}

// When the player is standing still.
if (keyboard_check_released(vk_left)) &amp;&amp; (place_free(x,y)){
sprite_index = spr_player_left2;
image_speed = 0.2;
}

