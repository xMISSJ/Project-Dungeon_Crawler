            <string>/// Menu Options Level Select

/* Draws tutorial, level I, level II and Level III buttons.
 This also assigns the menu options' positions. */
draw_sprite (spr_LS_tutorial, 0, x, y + 40);
draw_sprite (spr_LS_level1, 0, x + 5, y + 110);
draw_sprite (spr_LS_level2, 0, x + 3, y + 180);
draw_sprite (spr_LS_level3, 0, x + 4, y + 250);
draw_sprite (spr_LS_back, 0, x, y + 320);

// Makes it so the pointer can go down.
if (keyboard_check_pressed(vk_down)){
select++;
}

// Makes it so the pointer can go up.
if (keyboard_check_pressed(vk_up)){
select--;
}

/* Makes it so the pointer can select the menu options.
 Also assigns the position of the pointer for each menu option. */

switch (select){
case 0:

obj_select_r2.y = y + 25;
obj_select_l2.y = y + 25;


    if (keyboard_check_pressed(vk_enter)){
        audio_stop_sound(snd_music_menu);
        room_goto(room_pre_tutorial);
    }   
break;
case 1:
obj_select_r2.y = y + 95;
obj_select_l2.y = y + 95;

    if (keyboard_check_pressed(vk_enter)){
         room_goto(room_level1);     
}
break;
case 2:
obj_select_r2.y = y + 165;
obj_select_l2.y = y + 165;

    if (keyboard_check_pressed(vk_enter)){
         room_goto(room_level2);
    }
break;
case 3:
obj_select_r2.y = y + 235;
obj_select_l2.y = y + 235;
    if (keyboard_check_pressed(vk_enter)){
         room_goto(room_boss);
    }
break;
case 4:
obj_select_r2.y = y + 305;
obj_select_l2.y = y + 305;
    if (keyboard_check_pressed(vk_enter)){
        room_goto(room_menu);
    }
break;
default:
    if (select &gt; 4){
        select = 0;
    } 
    if (select &lt; 0){
        select = 4;
    }
break;
}
