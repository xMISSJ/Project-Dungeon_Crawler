          <argument>
            <kind>1</kind>
            <string>/// Keys for the Dialogue.

// This means: 
// If current character count is less than the amount in current message* 

if (characters &lt; message_length) { 
// Hold is true or false if we hold 'space' or not.
    hold = keyboard_check(ord(" ")); 
// This increases speed based on hold.
    characters += increase * (1 + hold); 
    // This copies string to current character.
    message_draw = string_copy(message[message_current], 0, characters); 
} 

// This means:
// If current character is more than the amount in the current message*
else {
// This is if the user presses space...
    if (keyboard_check_pressed(ord(" "))) { 
    //if there are more messages left to show (0 -&gt; 12, in this case).

        if (message_current &lt; message_end) {
        // This increases the message by 1.
            message_current += 1; 
            // This gets the new character length for message.
            message_length = string_length(message[message_current]);  
            // This sets the characters back to 0.
            characters = 0; 
            // This clears the drawn text.
            message_draw = ""; 
        }
  
    //if our messages are done (we reach 18, in this case)...
        else { 
        // This allows the player to go to the next room.
        room_goto(room_menu);
        // This destroys the object.
            instance_destroy(); 
        }
    }
}
</string>
